/boot           emmc    /dev/block/platform/sdhci.1/by-name/KERNEL
/system         ext4    /dev/block/platform/sdhci.1/by-name/system
/data           ext4    /dev/block/platform/sdhci.1/by-name/userdata                        length=-16384
/cache          ext4    /dev/block/platform/sdhci.1/by-name/CSC
/recovery       emmc    /dev/block/platform/sdhci.1/by-name/RECOVERY
/modem          emmc    /dev/block/platform/sdhci.1/by-name/modem                           flags=display="Baseband";backup=1
/efs            ext4    /dev/block/platform/sdhci.1/by-name/efs                             flags=display="EFS";backup=1
/external_sd    vfat    /dev/block/mmcblk1p1                            /dev/block/mmcblk1  flags=display="External storage";storage;wipeingui;removable
